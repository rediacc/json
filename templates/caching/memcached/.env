# Memcached Configuration
CONTAINER_NAME=memcached-server

# Memory allocation in megabytes (default: 64)
MEMCACHED_MEMORY=64

# Number of threads to use (default: 4)
# Recommended: Set to number of CPU cores (max 16)
MEMCACHED_THREADS=4

# Maximum number of simultaneous connections (default: 1024)
MEMCACHED_MAX_CONN=1024

# Verbosity level (optional)
# Leave empty for normal output
# Set to "v" for verbose, "vv" for very verbose
MEMCACHED_VERBOSITY=
